# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Build outputs
dist/
build/
.aws-sam/

# Prevent node_modules in dist/
dist/node_modules/
dist/package-lock.json
dist/package.json
dist/.npmrc

# Layer dependencies (generated)
layers/dependencies/nodejs/node_modules/
layers/dependencies/nodejs/package-lock.json

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
env.json

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
logs/
*.log

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# nyc test coverage
.nyc_output/

# Dependency directories
jspm_packages/

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Microbundle cache
.rpt2_cache/
.rts2_cache_cjs/
.rts2_cache_es/
.rts2_cache_umd/

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# parcel-bundler cache (https://parceljs.org/)
.cache
.parcel-cache

# Next.js build output
.next

# Nuxt.js build / generate output
.nuxt

# Gatsby files
.cache/
public

# Storybook build outputs
.out
.storybook-out

# Temporary folders
tmp/
temp/

# AWS SAM
.aws-sam/
samconfig.toml
parameters.json

# TypeScript
*.tsbuildinfo

# Database
*.db
*.sqlite
*.sqlite3

# Docker
.dockerignore

# Local development
local/
dev/

# Maven
target/

# Compiled Java class files
*.class

# Compiled Python bytecode
*.py[cod]

# Package files
*.jar

# Unit test reports
TEST*.xml

# Applications
*.app
*.exe
*.war

# Large media files
*.mp4
*.tiff
*.avi
*.flv
*.mov
*.wmv
